{
  "openapi": "3.0.4",
  "info": {
    "title": "CSharpApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Resume/generate": {
      "post": {
        "tags": [
          "Resume"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CandidateProfileDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CandidateProfileDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CandidateProfileDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CandidateProfileDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "mobileNo": {
            "type": "string",
            "nullable": true
          },
          "linkedIn": {
            "type": "string",
            "nullable": true
          },
          "gitHub": {
            "type": "string",
            "nullable": true
          },
          "cskills": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SkillDto"
            },
            "nullable": true
          },
          "educationList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EducationDto"
            },
            "nullable": true
          },
          "experienceList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExperienceDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EducationDto": {
        "type": "object",
        "properties": {
          "eduId": {
            "type": "integer",
            "format": "int32"
          },
          "degree": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "string",
            "nullable": true
          },
          "startYear": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "endYear": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "university": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ExperienceDto": {
        "type": "object",
        "properties": {
          "expId": {
            "type": "integer",
            "format": "int32"
          },
          "role": {
            "type": "string",
            "nullable": true
          },
          "companyName": {
            "type": "string",
            "nullable": true
          },
          "startDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "endDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "expInYears": {
            "type": "number",
            "format": "double"
          },
          "description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SkillDto": {
        "type": "object",
        "properties": {
          "skillId": {
            "type": "integer",
            "format": "int32"
          },
          "skillName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}